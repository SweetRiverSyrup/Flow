                        </div>
                        <div class="col-xs-1">
                            <button id="eight" class="ui-widget ui-button ui-corners-all">8</button>
                        </div>
                        <div class="col-xs-1">
                            <button id="nine" class="ui-widget ui-button ui-corners-all">9</button>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-1">
                            <button id="exp" class="ui-widget ui-button ui-corners-all">**</button>
                        </div>
                        <div class="col-xs-1">
                            <button id="four" class="ui-widget ui-button ui-corners-all">4</button>
                        </div>
                        <div class="col-xs-1">
                            <button id="five" class="ui-widget ui-button ui-corners-all">5</button>
                        </div>
                        <div class="col-xs-1">
                            <button id="six" class="ui-widget ui-button ui-corners-all">6</button>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-1">
                            <button id="zero" class="ui-widget ui-button ui-corners-all">0</button>
                        </div>
                        <div class="col-xs-1">
                            <button id="one" class="ui-widget ui-button ui-corners-all">1</button>
                        </div>
                        <div class="col-xs-1">
                            <button id="two" class="ui-widget ui-button ui-corners-all">2</button>
                        </div>
                        <div class="col-xs-1">
                            <button id="three" class="ui-widget ui-button ui-corners-all">3</button>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-4">
                            <button id="calc" class="ui-widget ui-button ui-corners-all ">
                                Enter
                            </button>
                        </div>
                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" id="displaySpecificGroup" name="displaySpecificGroup"
                        form="setGroup">Continue to Lab
                </button>
            </div>

            <script>
                $(function() {
                    $(".widget button").button();
                    $("#calc").css("width", '205px' ).click(function(){
                        var calculation = $('#calcinput').text().toString();
                            window.alert(eval(calculation));

                    });
                    $("#cls").click(function(){
                        $("#calcinput").val('');
                    });
                    var one = $("#one"),
                            two = $("#two"),
                            three = $("#three"),
                            four = $("#four"),
                            five = $("#five"),
                            six = $("#six"),
                            seven = $("#seven"),
                            eight = $("#eight"),
                            nine = $("#nine"),
                            plus = $("#plus"),
                            zero = $("#zero"),
                            minus = $("#minus"),
                            divide = $("#divide"),
                            multip = $("#multip"),
                            exp = $("#exp"),
                            paranth = $("#paranth");

                    one
                            .add(two)
                            .add(three)
                            .add(four)
                            .add(five)
                            .add(six)
                            .add(seven)
                            .add(eight)
                            .add(nine)
                            .add(plus)
                            .add(minus)
                            .add(divide)
                            .add(multip)
                            .add(exp)
                            .add(paranth)
                            .click(function(){
                                var whatwas = $("#calcinput").val();
                                var whatiam = $(this).text();
                                var whatis = `${whatwas}${whatiam}`;
                                $("#calcinput").val(whatis);
                            });
                    });


            </script>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -s-c -->


<script>
    $(document).ready(function () {
        $("#displaySpecificGroup").click(function () {
            var g = $("#groups2").val();
            switch (g) {
                case "1":
                    $("#g2, #g3, #g4, #g5").hide();
                    $("#g1").show().find("input, textarea, unit-sel").toggleAttr('required', true);
                    break;
                case "2":
                    $("#g1").hide();
                    $("#g2").show();
                    $("#g3").hide();
                    $("#g4").hide();
                    $("#g5").hide();
                    break;
                case "3":
                    $("#g1, #g2, #g4, #g5").hide();
                    $("#g3").show();
                    break;
                case "4":
                    $("#g1, #g2, #g3, #g5").hide();
                    $("#g4").show();
                    break;
                case "5":
                    $("#g1, #g2, #g3, #g4").hide();
                    $("#g5").show();
                    break;
                case "6":
                    $("#g1, #g2, #g3, #g4, #g5").show();
                    break;
                default:
                    $("#g1, #g2, #g3, #g4, #g5").find("input, textarea, unit-sel").toggleAttr('required', false);
            }
        });


    });
</script>

